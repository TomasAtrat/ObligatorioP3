@model IEnumerable<CommonSolution.Dto.DtoCuadrilla>

@{
    ViewBag.Title = "Cuadrillas más eficientes";
}

<div class="starter-template" style="margin-top: -20px">
    <h1>Cuadrillas más eficientes</h1>
</div>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.promedio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.encargado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cantidadPeones)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Observaciones)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.promedio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.encargado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.cantidadPeones)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Observaciones)
            </td>

        </tr>
    }

</table>
